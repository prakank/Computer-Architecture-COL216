main:
	addi $s0, $zero, 1000
	addi $s1, $zero, 1024
	addi $t0, $zero, 1
	addi $t1, $zero, 2
	
    sw $t0, 0($s0)
    
    addi $t4, $t4, 100
    addi $t4, $t4, 100
    addi $t4, $t4, 100
    addi $t0, $t0, 100
    addi $t4, $t4, 100
    addi $t4, $t4, 100
    addi $t4, $t4, 100

    sw $t0, 0($s1)
	
exit: